<html>
  <head>
    <meta charset="utf-8">
    <meta description="<%= htmlWebpackPlugin.options.title %>">
    <title><%= htmlWebpackPlugin.options.title %></title>
    <base href="<%= htmlWebpackPlugin.options.basehref %>">
    <%= htmlWebpackPlugin.options.inlineStyle %>
  </head>
  <body>
    <noscript>Please enable JavaScript to run this application.</noscript>
    <app-root>
      <div class="placeholder">
        <svg width="180" height="60" preserveAspectRatio="xMinYMin meet" style="transform: scale(1.5);">
          <use href="#svgplaceholder"/>
        </svg>
      </div>
    </app-root>
    <div style="display: none; visibility: hidden;">
      <%= htmlWebpackPlugin.options.placeholder %>
    </div>
  </body>
</html>
